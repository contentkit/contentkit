{
  "name": "contentkit",
  "regions": [
    "us-west-2"
  ],
  "lambda": {
    "memory": 1536
  },
  "type": "static",
  "static": {
    "dir": "dist",
    "prefix": "/assets/"
  },
  "redirects": {
    "/*": {
      "location": "/",
      "status": 200
    }
  },
  "stages": {
    "staging": {
      "domain": "dev.contentkit.co",
      "zone": "contentkit.co"
    },
    "production": {
      "domain": "contentkit.co",
      "zone": "contentkit.co"
    }
  }
}
