{
  "name": "contentkit",
  "version": "0.0.21",
  "main": "dist",
  "scripts": {
    "clean": "rimraf .cache && rimraf dist && rimraf lib",
    "dev": "UP_STAGE='/staging' parcel src/index.html",
    "watch": "babel src -d lib --copy-files --watch",
    "build": "NODE_ENV=production parcel build src/index.html --public-url /",
    "build:up": "parcel build src/index.html --public-url /assets/ --out-dir ./up/dist",
    "lint": "standard \"src/**/*.js\" --verbose --fix | snazzy"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.49",
    "@material-ui/core": "^1.1.0",
    "@material-ui/icons": "^1.1.0",
    "apollo-cache-inmemory": "^1.2.0",
    "apollo-client": "^2.3.0",
    "apollo-link": "^1.2.0",
    "apollo-link-http": "^1.5.4",
    "apollo-link-ws": "^1.0.6",
    "apollo-utilities": "^1.0.8",
    "aws-sdk": "^2.218.1",
    "date-fns": "^1.29.0",
    "dotenv": "^6.0.0",
    "draft-js": "^0.10.5",
    "draft-js-compact": "0.0.4",
    "express": "^4.16.3",
    "graphql": "^0.13.2",
    "graphql-request": "^1.5.1",
    "graphql-tag": "^2.9.2",
    "haikunator": "^2.0.0",
    "immutable": "~3.7.4",
    "jsondiffpatch": "^0.3.10",
    "lodash.debounce": "^4.0.8",
    "lodash.escape": "^4.0.1",
    "lodash.isequal": "^4.5.0",
    "lodash.memoize": "^4.1.2",
    "lodash.unescape": "^4.0.1",
    "monograph": "^0.0.26",
    "prismjs": "^1.14.0",
    "prop-types": "^15.5.10",
    "query-string": "^6.0.0",
    "react": "16.4.0-alpha.0911da3",
    "react-apollo": "^2.1.4",
    "react-dates": "^16.6.1",
    "react-dom": "16.4.0-alpha.0911da3",
    "react-inspector": "^2.3.0",
    "react-keydown": "^1.7.4",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.1.2",
    "recompose": "^0.27.0",
    "redux": "^4.0.0",
    "simple-cache-provider": "^0.5.0",
    "subscriptions-transport-ws": "^0.9.7",
    "with-async-component": "^0.0.7"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "node_modules",
      "dist",
      ".cache",
      "src/__test__",
      "src/__store__"
    ],
    "plugins": [
      "flowtype"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint-plugin-flowtype": "^2.49.3",
    "execa": "^0.10.0",
    "flow-bin": "^0.73.0",
    "flow-remove-types": "^1.2.3",
    "immutablediff": "^0.4.4",
    "jest": "^22.4.3",
    "nightmare": "^3.0.1",
    "node-sass": "^4.9.0",
    "parcel-bundler": "1.9.1",
    "pushstate-server": "^3.0.1",
    "react-addons-perf": "^15.4.2",
    "react-test-renderer": "^16.3.2",
    "sinon": "^5.0.7",
    "snazzy": "^7.0.0",
    "standard": "^11.0.0-beta.0"
  }
}
